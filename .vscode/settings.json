{
    "IDX.aI.enableInlineCompletion": true,
    "IDX.aI.enableCodebaseIndexing": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/checkpoints/**": true,
        "**/data/**": true,
        "**/*.db": true,
        "**/*.pt": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/checkpoints": true,
        "**/data": true,
        "**/*.db": true,
        "**/*.pt": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true
    }
}